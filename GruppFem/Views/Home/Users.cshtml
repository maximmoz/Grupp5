@model  List<GruppFem.ServiceReference1.UserInfo>
@{
    /**/

    /**/

    /**/

    /**/

    ViewBag.Title = "Users";
}

@foreach (var item in Model)
{

    using (Html.BeginForm())
    {
        @Html.Hidden("userID", item.ID);
        @Html.TextBox("username", item.Username);
        @Html.TextBox("password", item.Password);
        @Html.TextBox("firstname", item.FName);
        @Html.TextBox("lastname", item.LName);
        @Html.TextBox("email", item.Email);
        <input type="submit" value="Update">





        @Html.ActionLink("Delete", "DeleteUser", new { userID = item.ID });

    }
}